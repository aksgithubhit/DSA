int ans=-1;
        int target=n;
        int s=0;
        int e=n;
        int mid=s+(e-s)/2;
        while(s<=e){
            if(mid*mid <=target){
                ans=mid;
                s=mid+1;
            }
            // left search
            else {
                e=mid-1;
            }
            
            mid=s+(e-s)/2;
        }
        return ans;
